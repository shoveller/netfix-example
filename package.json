{
  "name": "netfix-example",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "format": "prettier --loglevel warn --write \"**/*.{ts,tsx,css,md}\"",
    "posttest": "npm run format",
    "lint:css": "stylelint '**/*.css'",
    "pretest": "npm run lint:css",
    "prepare": "husky install",
    "build-storybook": "build-storybook",
    "push-storybook": "gh-pages -d storybook-static -b gh-pages -r https://github.com/shoveller/netfix-example-storybook",
    "publish-storybook": "run-s build-storybook push-storybook",
    "build": "tsc && vite build",
    "push-library": "gh-pages -d dist -b gh-pages -r https://github.com/shoveller/netfix-example-cdn",
    "publish-library": "run-s build push-library"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/html": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/eslint": "^8.4.10",
    "@types/jquery": "^3.5.16",
    "@types/jqueryui": "^1.12.16",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@whitespace/storybook-addon-html": "^5.1.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.33.0",
    "eslint-config-plugin": "^1.0.11",
    "gh-pages": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "path-to-regex": "^1.3.8",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "react-syntax-highlighter": "^15.5.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-barrels": "^1.0.1",
    "vite-tsconfig-paths": "^4.0.5"
  }
}
